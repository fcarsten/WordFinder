<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="15"
	android:versionName="4.02"
    android:installLocation="auto"
	>

	<uses-permission android:name="android.permission.VIBRATE" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

	<application
		android:icon="@mipmap/ic_launcher"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:label="@string/app_name"
		android:theme="@style/Theme.WordFinder"
		android:supportsRtl="false"
		android:allowBackup="true">
		<activity android:name="org.carstenf.wordfinder.WordFinder" android:exported="true">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="org.carstenf.wordfinder.WordFinderPreferences"
			android:exported="false"
			android:label="@string/set_preferences">
		</activity>
	</application>
</manifest>
